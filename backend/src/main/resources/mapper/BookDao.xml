        <result property="sort" column="sort" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
        <result property="doubanUrl" column="douban_url" jdbcType="VARCHAR"/>
        <result property="coverUrl" column="cover_url" jdbcType="VARCHAR"/>
        <result property="measure" column="measure" jdbcType="VARCHAR"/>
        <result property="framing" column="framing" jdbcType="VARCHAR"/>
        <result property="series" column="series" jdbcType="VARCHAR"/>
        <result property="translator" column="translator" jdbcType="VARCHAR"/>
        <result property="price" column="price" jdbcType="NUMERIC"/>
        <result property="pages" column="pages" jdbcType="INTEGER"/>
        <result property="publication" column="publication" jdbcType="TIMESTAMP"/>
